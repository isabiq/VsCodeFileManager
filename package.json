{
  "name": "FileManager",
  "displayName": "Vscode File Manager",
  "description": "Simple UI file navigator",
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode",
    "file",
    "files",
    "manager",
    "filemanager",
    "filesystem",
    "system"
  ],
  "publisher": "ArtTemiy",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "repository": "git@github.com:ArtTemiy/FileManagerVsCodeExtension.git",
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.46.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "FileManager.openFileManager",
        "title": "Open file manager",
        "category": "FileManager"
      }
    ],
    "configuration": {
      "title": "fileManager",
      "type": "object",
      "properties": {
        "fileManager": {
          "type": "object",
          "items": {
            "type": "object",
            "title": "test test",
            "properties": {
              "maxSize": {
                "type": "number",
                "default": "5242880"
              },
              "maxCharacters": {
                "type": "number",
                "default": "1000000"
              },
              "excludeExtensions": {
                "type": "array",
                "default": [
                  ".zip",
                  ".gz",
                  ".xz",
                  ".tgz",
                  ".deb",
                  ".pdf",
                  ".docx",
                  ".epub",
                  ".mp4"
                ]
              }
            }
          }
        }
      }
    },
    "viewsWelcome": [
      {
        "view": "workbench.explorer.emptyView",
        "contents": "You can have paragraphs of text here. You can have [links](https://code.visualstudio.com) to external sources or [internal commands](command:FileManager.hello).\nUse new lines to have new paragraphs.\nPlace a link alone in a paragraph to make it a button\n[Hello](command:FileManager.hello)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "rollup -c",
    "compile-watch": "rollup -c -w",
    "watch": "",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/glob": "^7.1.3",
    "@types/node": "^16.11.56",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/codicons": "^0.0.32",
    "@vscode/test-electron": "^1.6.1",
    "classnames-ts": "^1.0.2",
    "eslint": "^8.13.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "node-sass": "^7.0.1",
    "prettier": "^2.7.1",
    "pretty-bytes": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "rollup": "^2.79.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.8.2"
  }
}